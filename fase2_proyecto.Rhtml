          <p><b>Consultas SQL realizadas:</b></p>

          <pre><code>
-- 1. Productos por secci√≥n
SELECT section, COUNT(*) AS num_productos
FROM zara_sales
GROUP BY section;

-- 2. Ventas por temporada
SELECT season, AVG(sales), SUM(sales)
FROM zara_sales
GROUP BY season;

-- 3. Ventas por rangos de precio
SELECT CASE
  WHEN price < 20 THEN 'Bajo'
  WHEN price BETWEEN 20 AND 39.99 THEN 'Medio'
  ELSE 'Alto' END AS rango_precio,
  AVG(sales), COUNT(*)
FROM zara_sales
GROUP BY rango_precio;

-- 4. Promoci√≥n vs ventas
SELECT promotion, AVG(sales), SUM(sales)
FROM zara_sales
GROUP BY promotion;

-- 5. Ventas por origen
SELECT origin, AVG(sales), SUM(sales)
FROM zara_sales
GROUP BY origin;

-- 6. Ventas por posici√≥n en tienda
SELECT product_position, AVG(sales), COUNT(*)
FROM zara_sales
GROUP BY product_position;

-- 7. Ventas por terms (colecci√≥n)
SELECT terms, AVG(sales), SUM(sales)
FROM zara_sales
GROUP BY terms;
          </code></pre>

